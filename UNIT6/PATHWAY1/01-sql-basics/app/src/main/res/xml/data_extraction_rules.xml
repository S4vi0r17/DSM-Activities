<?xml version="1.0" encoding="utf-8"?><!--
     Backup rules for Android Auto Backup. This file specifies files that will be
     backed up and restored by Android Auto Backup.
-->
<data-extraction-rules>
    <cloud-backup>
        <include domain="sharedpref" path="."/>
        <include domain="database" path="."/>
        <include domain="root" path="."/>
    </cloud-backup>
</data-extraction-rules>
